{
    "id": "create_folder_module",
    "name": "Smart Create Folder",
    "version": "1.2.0",
    "icon_file": "icon.gif",
    "author": "awenk audico ",
    "email": "sahidinaola@gmail.com",
    "website": "www.teetah.art",
    "description": "Creates a new folder. The folder name can be set manually or dynamically taken from a previous node's output.",
    "type": "ACTION",
    "entry_point": "processor.CreateFolderModule",
    "tier": "free",
    "behaviors": [
        "retry",
        "loop"
    ],
    "permissions": [],
    "requires_services": [
        "logger",
        "loc",
        "kernel"
    ],
    "properties": [
        {
            "id": "parent_path",
            "type": "string",
            "label": "loc.prop_parent_path_label"
        },
        {
            "id": "folder_name_mode",
            "type": "enum",
            "options": ["manual", "auto"],
            "label": "loc.prop_foldername_mode_label"
        }
    ],
    "output_schema": [
        {
            "name": "data.created_path",
            "type": "string",
            "description": "The full path of the newly created folder."
        }
    ]
}