{
    "id": "function_runner_module",
    "name": "Run Python Code (Nano)",
    "version": "1.1",
    "icon_file": "icon.png",
    "author": "Flowork Contributor",
    "email": "Contributor@teetah.art",
    "website": "www.teetah.art",
    "description": "Executes a simple Python code snippet directly to manipulate the payload. The foundation of the Nano-Modular concept.",
    "type": "LOGIC",
    "entry_point": "processor.FunctionRunnerModule",
    "tier": "free",
    "requires_input": true,
    "behaviors": [
        "retry",
        "loop"
    ],
    "properties": [
        {
            "id": "python_code",
            "type": "code_editor",
            "label": "loc.prop_python_code_label",
            "description": "loc.prop_python_code_desc",
            "default": "# 'payload' dictionary is available here\n# Example: payload['data']['new_data'] = 'Hello from Nano Module!'\nreturn payload"
        }
    ],
    "output_ports": [
        {
            "name": "success",
            "display_name": "loc.port_success"
        },
        {
            "name": "error",
            "display_name": "loc.port_on_failure"
        }
    ],
    "requires_services": [
        "logger",
        "loc",
        "kernel"
    ],
    "display_properties": {
        "color": "#009688",
        "text_color": "#FFFFFF"
    }
}