{
    "id": "dictionary_upsert_1998",
    "name": "Dictionary Upsert",
    "version": "1.0",
    "icon_file": "icon.png",
    "author": "Flowork Contributor",
    "email": "contributor@teetah.art",
    "website": "https://www.teetah.art",
    "description": "Adds or updates a key-value pair in a target dictionary within the payload.",
    "type": "ACTION",
    "entry_point": "processor.DictionaryUpsert1998Module",
    "behaviors": [
        "loop",
        "retry"
    ],
    "requires_services": [
        "logger",
        "loc"
    ],
    "properties": [
        {
            "id": "target_dictionary_path",
            "type": "string",
            "label": "Path to Target Dictionary",
            "default": "data.variables"
        },
        {
            "id": "key_to_upsert",
            "type": "string",
            "label": "Key to Add/Update",
            "default": ""
        },
        {
            "id": "value_to_upsert",
            "type": "string",
            "label": "Value to Set",
            "default": ""
        }
    ],
    "output_ports": [
        {
            "name": "success",
            "display_name": "Success"
        },
        {
            "name": "error",
            "display_name": "Error"
        }
    ]
}