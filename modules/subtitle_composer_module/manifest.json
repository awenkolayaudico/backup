{
    "id": "subtitle_composer_module",
    "name": "Subtitle Composer",
    "version": "1.0.0",
    "icon_file": "icon.png",
    "author": "AWENK AUDICO & Gemini",
    "email": "contributor@teetah.art",
    "website": "www.teetah.art",
    "description": "Creates a structured subtitle list from manual text inputs, complete with timestamps.",
    "type": "LOGIC",
    "entry_point": "processor.SubtitleComposerModule",
    "tier": "free",
    "behaviors": [
        "retry",
        "loop"
    ],
    "requires_services": [
        "logger",
        "loc"
    ],
    "properties": [
        {
            "id": "subtitle_entries",
            "type": "custom",
            "label": "loc.prop_subtitle_entries_label"
        }
    ],
    "output_ports": [
        {
            "name": "success",
            "display_name": "loc.port_success"
        }
    ],
    "output_schema": [
        {
            "name": "data.subtitle_data",
            "type": "list",
            "description": "A list of subtitle objects, each with start time, end time, and text."
        }
    ]
}