{
    "id": "Sub-Workflow",
    "name": "Sub-Workflow Configuration",
    "version": "1.1.0",
    "icon_file": "icon.PNG",
    "author": "awenk audico",
    "email": "sahidinaola@gmail.com",
    "website": "www.teetah.art",
    "description": "Executes one or more other workflows (from presets) as part of the main workflow sequentially.",
    "type": "CONTROL_FLOW",
    "entry_point": "processor.SubWorkflowModule",
    "tier": "free",
    "behaviors": [
        "retry",
        "loop"
    ],
    "requires_input": true,
    "requires_services": [
        "kernel",
        "loc",
        "logger"
    ],
    "output_ports": [
        {
            "name": "success",
            "display_name": "loc.port_subworkflow_done"
        }
    ],
    "output_schema": [
        {
            "name": "output",
            "type": "any",
            "description": "The payload generated by the last sub-workflow that was executed."
        }
    ]
}